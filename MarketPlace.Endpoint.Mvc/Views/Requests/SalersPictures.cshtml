        $.ajax({
            type: "patch",
            url: `/Requests/Confirm`+ type+ `Pic/` + id,
            data: { id: id },
            dataType: "JSON",
            success: function (response) {
                console.log(response)
                //console.log(response.carts[3].products);
                DeletTr(id,type);
            },
        });
        DeletTr(id, type);
    }
    function Fale(id,type) {
